{
  "name": "@vandebron/windmolen-web",
  "version": "2.0.1-5.16",
  "description": "vandebron's styleguide for web",
  "main": "dist/bundle.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean-dist": "rimraf dist/**",
    "copy-assets": "cd src && cpy --parents assets/ ../dist/",
    "build": "yarn clean-dist && yarn copy-assets && rollup -c rollup.config.js",
    "lint": "eslint -c .eslintrc.json 'src/**/*.js'",
    "test": "jest",
    "website:start": "cross-env HTTPS=false docz dev",
    "website:build": "docz build",
    "prepack": "yarn build"
  },
  "keywords": [
    "vandebron",
    "styleguide",
    "web"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "docz": "^0.9.6",
    "eslint": "^5.3.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-react": "^7.11.0",
    "flow-bin": "^0.78.0",
    "flow-typed": "^2.5.1",
    "jest": "^23.5.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-svgo": "^1.0.1",
    "rollup-plugin-terser": "^1.0.1",
    "styled-components": "^3.4.2",
    "svg-inline-loader": "^0.8.0"
  },
  "peerDependencies": {
    "react": "^16.4.2"
  },
  "gitHead": "d90b3be863a48d73d179fd4ef1b8d007d2767df0"
}
